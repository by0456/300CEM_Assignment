<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/MyPlanFragment">

    <fragment
        android:id="@+id/MyPlanFragment"
        android:name="com.assignment.travelplanner.MyPlanFragment"
        android:label="@string/myPlan_fragment_label"
        tools:layout="@layout/fragment_my_plan">


        <action
            android:id="@+id/action_MyPlanFragment_to_SecondFragment"
            app:destination="@id/SecondFragment" />
    </fragment>
    <fragment
        android:id="@+id/SecondFragment"
        android:name="com.assignment.travelplanner.SecondFragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_second">


        <action
            android:id="@+id/action_SecondFragment_to_MyPlanFragment"
            app:destination="@id/MyPlanFragment" />
    </fragment>

</navigation>
